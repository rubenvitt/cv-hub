{
  "name": "@cv-hub/shared-types",
  "version": "0.1.0",
  "description": "Shared TypeScript Types and Zod Schemas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint 'src/**/*.ts'",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "types",
    "zod",
    "shared"
  ],
  "author": "Ruben",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "vitest": "^4.0.8"
  }
}
